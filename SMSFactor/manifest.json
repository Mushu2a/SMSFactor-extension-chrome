{
	"manifest_version" : 2,
	"name" : "SMSFactor",
	"version" : "0.2",
	"content_security_policy" : "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"description" : "Extension chrome SMSFactor pour l'envois de SMS",
	"options_page" : "options_smsfactor.html",
	"background":{
		"scripts" : ["background.js"]
	},
	"permissions": ["storage"],
	"icons": {
		"16" : "SMSFactor.png",
		"48" : "SMSFactor.png",
		"128" : "SMSFactor.png"
	},
	"browser_action" : {
		"default_icon" : "SMSFactor.png",
		"default_popup" : "popup_smsfactor.html"
	}
}